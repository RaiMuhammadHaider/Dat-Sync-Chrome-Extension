{
  "manifest_version": 3,
  "name": "DAT Sync 2.0",
  "version": "2.1.0",
  "description": "Sync and clone session cookies via GitHub Gists. Generate Base64 setup token for DAT Access.",
  "action": {
    "default_title": "DAT Sync 2.0",
    "default_popup": "popup.html"
  },
  "permissions": ["cookies", "storage", "activeTab", "alarms"],
  "host_permissions": ["<all_urls>", "https://api.github.com/*"],
  "background": {
    "service_worker": "worker.js"
    
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
